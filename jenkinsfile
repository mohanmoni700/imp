pipeline {

    agent any

    stages {
            stage('stage1-download') {
                
                steps {
                    sh '''
                    echo mohan
                    '''
                    
                }
            }    
            
            stage('stage2-testing') {
                 
                steps {
                    
                    catchError(buildResult: 'SUCCESS', stageResult: 'FAILURE') {
                            sh '''
                            echo testing
                            exit 1
                            '''
                            
                     }
                }
            }

            stage('stage3-deploy') {
                steps {
                 sh '''
                 echo deploy

                 '''
                }
            }

    }


}
